{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSourceMap": true,
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../assets/javascripts",
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "test/**/*.test.ts"
  ]
}